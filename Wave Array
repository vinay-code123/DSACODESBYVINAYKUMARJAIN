void convertToWave(int n, vector<int>& a){
     if(n%2 != 0){
            for(int i=0;i<n-1;i++){
                int t = a[i];
                a[i] = a[i+1];
                a[i+1] = t;
                i++;
            }
        }
        else{
            for(int i=0;i<n;i++){
                int t = a[i];
                a[i] = a[i+1];
                a[i+1] = t;
                i++;
            }
        }
    }
