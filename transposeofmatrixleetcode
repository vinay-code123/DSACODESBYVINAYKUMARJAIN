class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& matrix) {
          int n = matrix.size();
        int m = matrix[1].size();
        
        vector<vector<int>> new1(m,vector<int> (n)); 
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                new1[j][i] = matrix[i][j];
            }
        }
        return new1;
        
    }
};
